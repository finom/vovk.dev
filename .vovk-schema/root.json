{
  "$schema": "https://vovk.dev/api/spec/v3/segment.json",
  "emitSchema": true,
  "segmentName": "",
  "segmentType": "segment",
  "controllers": {
    "V3SSchemaRPC": {
      "rpcModuleName": "V3SSchemaRPC",
      "originalControllerName": "V3SSchemaController",
      "prefix": "v3",
      "handlers": {
        "getSegmentDefinition": {
          "path": "segment.json",
          "httpMethod": "GET"
        },
        "getConfigDefinition": {
          "path": "config.json",
          "httpMethod": "GET"
        },
        "getMetaDefinition": {
          "path": "meta.json",
          "httpMethod": "GET"
        },
        "getFullDefinition": {
          "path": "schema.json",
          "httpMethod": "GET"
        }
      }
    }
  }
}